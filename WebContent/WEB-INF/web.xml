<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">
	<display-name>StorageDemo</display-name>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<display-name>Register</display-name>
		<servlet-name>Register</servlet-name>
		<servlet-class>com.cloudthat.servlets.Register</servlet-class>
	</servlet>
	
	<servlet>
		<display-name>UserOperations</display-name>
		<servlet-name>UserOperations</servlet-name>
		<servlet-class>com.cloudthat.servlets.UserOperations</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Register</servlet-name>
		<url-pattern>/Register</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UserOperations</servlet-name>
		<url-pattern>/UserOperations</url-pattern>
	</servlet-mapping>
	

</web-app>